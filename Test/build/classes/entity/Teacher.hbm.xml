<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="entity.Teacher" table="teacher">
		<id name="tid" type="java.lang.String" length="8">
			<generator class="assigned"></generator>
		</id>
		<property name="tname" type="java.lang.String"></property>
		<property name="gender" type="java.lang.String"></property>
	    <property name="birthday" type="date"></property>
	    <property name="address" type="java.lang.String"></property>
	
	</class>
</hibernate-mapping>